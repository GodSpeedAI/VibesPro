{
  "name": "vibes-pro",
  "version": "0.6.0",
  "description": "Unified generator-first platform combining HexDDD and VibePDK",
  "type": "module",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "test:jest": "jest --config jest.config.json",
    "test:jest:watch": "jest --config jest.config.json --watch",
    "test:jest:coverage": "jest --config jest.config.json --coverage",
    "test:integration": "jest --config jest.config.json --testPathPatterns=tests/integration",
    "test:integration:watch": "jest --config jest.config.json --testPathPatterns=tests/integration --watch",
    "test:e2e": "jest --config jest.config.json tests/integration/end-to-end.test.ts --timeout=300000",
    "lint": "nx run-many --target=lint --all",
    "typecheck": "sh tools/check_mypy.sh",
    "dev": "nx run-many --target=serve --all --parallel=5",
    "generate": "copier copy . ../test-output",
    "test-generation": "copier copy . ../test-output && cd ../test-output && pnpm install && pnpm build",
    "spec:matrix": "node tools/spec/matrix.js",
    "prompt:lint": "for f in .github/prompts/*.prompt.md; do node tools/prompt/lint.js \"$f\" || exit 1; done",
    "prompt:plan": "for f in .github/prompts/*.prompt.md; do node tools/prompt/plan_preview.js \"$f\" || exit 1; done",
    "prompt:plan:accurate": "for f in .github/prompts/*.prompt.md; do PROMPT_TOKENIZER=accurate node tools/prompt/plan_preview.js --accurate \"$f\" || exit 1; done",
    "lint:md": "markdownlint \"**/*.md\" --config .markdownlint.json --ignore node_modules --ignore .nx --ignore dist --ignore coverage --ignore libs/tools/spec-kit",
    "lint:shell": "shellcheck scripts/*.sh || echo 'shellcheck not installed, skipping shell linting'",
    "docs:links": "node tools/docs/link_check.js",
    "env:audit": "node tools/audit/env_audit.js",
    "format": "nx format --all",
    "pr:comment": "node tools/ci/pr_comment.js",
    "test:node": "node tools/test/node-smoke.cjs"
  },
  "workspaces": [
    "templates/*",
    "tools/*",
    "generators/*"
  ],
  "devDependencies": {
    "@babel/runtime": "~7.27.6",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^2.0.0",
    "@eslint/js": "~8.57.0",
    "@expo/cli": "~0.24.14",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/devkit": "^22.0.2",
    "@nx/eslint-plugin": "22.0.4",
    "@nx/expo": "^22.0.2",
    "@nx/jest": "^22.0.2",
    "@nx/js": "22.0.4",
    "@nx/next": "^22.0.4",
    "@nx/node": "22.0.2",
    "@nx/react": "^22.0.2",
    "@nx/remix": "^22.0.4",
    "@nx/vite": "22.0.4",
    "@nx/web": "22.0.4",
    "@nx/workspace": "22.0.2",
    "@nxlv/python": "^21.2.0",
    "@openapitools/openapi-generator-cli": "^2.25.1",
    "@phenomnomnominal/tsquery": "^6.1.3",
    "@remix-run/dev": "^2.15.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.5.2",
    "@types/react": "~19.2.7",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "ajv": "^8.17.1",
    "babel-preset-expo": "~13.2.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "^16.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "glob": "^11.1.0",
    "jest": "^30.1.3",
    "jest-extended": "^6.0.0",
    "js-yaml": "^4.1.1",
    "json-schema-to-typescript": "^15.0.4",
    "markdownlint": "^0.36.0",
    "markdownlint-cli": "^0.36.0",
    "metro-config": "~0.82.4",
    "metro-resolver": "~0.82.4",
    "next": "^16.0.0",
    "nx": "22.0.2",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.4",
    "ts-morph": "^27.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "~5.9.2",
    "vite": "^5.0.0",
    "yaml": "^2.8.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.1.0",
    "pnpm": ">=8.0.0"
  },
  "overrides": {
    "vite-node": "1.6.1"
  },
  "packageManager": "pnpm@9.15.4",
  "pnpm": {
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "estree-util-value-to-estree@<3.3.3": ">=3.3.3",
      "vite@>=5.2.6 <=5.4.20": ">=5.4.21",
      "koa@>=3.0.1 <3.0.3": ">=3.0.3",
      "js-yaml@<3.14.2": ">=3.14.2",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "@vanilla-extract/integration>esbuild": ">=0.25.0"
    },
    "patchedDependencies": {}
  },
  "dependencies": {
    "@expo/metro-config": "~0.20.14",
    "@expo/metro-runtime": "~5.0.4",
    "@remix-run/node": "^2.15.0",
    "@remix-run/react": "^2.15.0",
    "@remix-run/serve": "^2.15.0",
    "expo": "~53.0.10",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.8",
    "isbot": "^4.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.0.0",
    "react-native": "0.79.3",
    "react-native-svg": "~15.11.2",
    "react-native-svg-transformer": "~1.5.1",
    "react-native-web": "~0.20.0",
    "tslib": "^2.3.0",
    "zod": "^4.1.12"
  }
}
