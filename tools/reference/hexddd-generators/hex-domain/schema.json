{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "HexDomain",
  "title": "Hex Domain Generator",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the domain.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the name of the domain?"
    }
  },
  "required": ["name"]
}
