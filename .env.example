#
# Copy this file to .env.local (or your preferred dotenv file) and replace the placeholder
# values with project-specific secrets before running the Supabase dev stack.
#

# Required by docker/docker-compose.supabase.yml
POSTGRES_PASSWORD=replace-with-strong-password
JWT_SECRET=replace-with-jwt-secret
ANON_KEY=replace-with-anon-key
SERVICE_ROLE_KEY=replace-with-service-role-key
GOTRUE_UI_ADMIN_EMAIL=admin@example.com
GOTRUE_UI_ADMIN_PASSWORD=replace-with-strong-password
LOGFLARE_API_KEY=replace-with-logflare-api-key

# Optional health endpoint override for Supabase smoke tests
SUPABASE_HEALTH_URL=http://localhost:54323
