# Copy this file to .env.local and adjust the values for your environment.
#
# The credentials below are development defaults only. Use unique values for
# every local workspace and NEVER commit the derived .env.local file.

# Root credentials (used to mint API tokens)
ZO_ROOT_USER_EMAIL=root@example.com
# Always set a unique, strong password before starting OpenObserve locally.
# Leave blank here to force a runtime override via secrets or `export`.
ZO_ROOT_USER_PASSWORD=

# Ports exposed on the host for HTTP UI and OTLP gRPC
OPENOBSERVE_HTTP_PORT=5080
OPENOBSERVE_GRPC_PORT=5081

# Optional: pre-create an organization name (defaults to "default")
ZO_ROOT_ORG=default
